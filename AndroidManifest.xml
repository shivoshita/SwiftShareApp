<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SwiftShareApp"
        tools:targetApi="35">

        <activity
            android:name=".HomeActivity"
            android:exported="true"
            android:noHistory="true"
            android:theme="@style/Theme.SwiftShareApp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".BookingActivity" />

        <activity
            android:name=".CabOptionsActivity" />

        <activity
            android:name=".AutoOptionsActivity" />

        <activity
            android:name=".CabRideSelectedActivity" />

        <activity
            android:name=".AutoRideSelectedActivity" />

        <activity
            android:name=".PeopleNearActivity"/>

        <activity
            android:name=".LastActivity"/>
    </application>
</manifest>
